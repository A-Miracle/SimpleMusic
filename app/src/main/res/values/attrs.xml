<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="ate_key" format="string" />

    <declare-styleable name="ScanView">
        <attr name="circleColor" format="color"/>
        <attr name="radarColor" format="color"/>
        <attr name="degree" format="integer"/>
        <attr name="rate" format="integer"/>
        <attr name="direction" format="enum">
            <enum name="clockwise" value="1" />
            <enum name="counterclockwise" value="-1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ATEPreference">
        <attr name="ateKey_pref" format="string" />
    </declare-styleable>
</resources>